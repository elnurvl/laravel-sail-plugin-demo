{
    "name": "elnurvl/sail-plugin",
    "description": "A utility plugin the fork of Laravel Sail (elnurvl/sail)",
    "keywords": ["laravel", "sail", "nginx", "keycloak"],
    "type": "library",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "elnurvl/sail": "^0.1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "elnurvl\\SailPlugin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "elnurvl\\SailPlugin\\ServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Elnur Hajiyev"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
